{
  "name": "video-animals-stories-maker",
  "version": "2.0.0",
  "description": "TypeScript pipeline for generating multilingual animal videos with voiceovers, captions, and cloud rendering.",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "video",
    "automation",
    "ai",
    "openai"
  ],
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "aws-sdk": "^2.1693.0",
    "axios": "^1.7.9",
    "dotenv": "^16.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.2",
    "json2video-sdk": "^2.0.4",
    "openai": "^5.3.0",
    "replicate": "^1.4.0"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.13.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
